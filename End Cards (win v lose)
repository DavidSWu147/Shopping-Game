import pygame, math, sys
from pygame import *

class EndCards:

  def __init__(self):
    if InputBudget(self) - sum[cart] >= 0:
      print("Congratualations! You've won! Great job staying focused and on budget.")
    if InputBudget(self) - sum[cart] < 0:
      print("You went over budget! Please select which items you would like to remove from your cart.")
